<div class="container-fluid" id="samagri-container">
	{{#each panels}}
	<div class="col-md-3 col-xs-12 samagri-panel">
		<div class="samagri-panel-header" style="height: 150px;">
			<h1>{{title}}</h1>
		</div>
		<div class="samagri-panel-circle">
			{{#if link}}
			<a class="samagri-link" href="{{link}}">{{link_title}}</a>
			{{/if}}
		</div>
		<table class="table samagri-table">
			<tbody style="height: 220px;">
				{{#each rows}}
				<tr>
					<td>{{column_one}}</td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
	{{/each}}
</div>
