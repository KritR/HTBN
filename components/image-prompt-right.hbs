{{#with data}}
<div class="image-prompt-right container-fluid">
  <div class="row">
    <img src="{{image}}"></img>
  </div>
  <div class="row prompt">
    <div class="col-md-8 col-sm-6 hidden-sm-down"></div>
    <div class="col-md-4 col-sm-6 col-xs-12">
      <div class="content-block">
        <h1 class="ul-left">{{title}}</h1>
        <p>{{text}}</p>
        <a class="lead" href="{{link}}">{{button}}</a>
      </div>
    </div>
  </div>
</div>
{{/with}}
